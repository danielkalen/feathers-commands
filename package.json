{
  "name": "feathers-commands",
  "version": "0.1.0",
  "description": "Create custom commands/routes for services beyond the 6 basic methods provided by FeathersJS",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run test -s",
    "postpublish": "git push",
    "test": "ava -s --verbose"
  },
  "ava": {
    "files": [
      "test/test.coffee"
    ],
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "coffee"
    ],
    "require": [
      "coffee-register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielkalen/feathers-commands.git"
  },
  "author": "danielkalen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielkalen/feathers-commands/issues"
  },
  "homepage": "https://github.com/danielkalen/feathers-commands#readme",
  "keywords": [
    "feathers",
    "feathers-plugin"
  ],
  "dependencies": {
    "@feathersjs/commons": "^1.4.1",
    "@feathersjs/errors": "^3.3.0",
    "coffee-register": "^0.1.3",
    "smart-extend": "^1.7.3"
  },
  "devDependencies": {
    "@feathersjs/express": "^1.2.2",
    "@feathersjs/feathers": "^3.1.5",
    "ava": "^1.0.0-beta.5.1",
    "supertest": "^3.0.0"
  }
}
